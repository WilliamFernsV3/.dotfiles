{
    "Add trace message": {
        "prefix": "trace",
        "body": [
            "logger.trace(",
            "\t\"${2:Message}.\",",
            "\tmetadata: [",
            "\t\t\"to\": .string(\"\\(String(describing: Self.self)).\\(#function)\"),",
            "\t\t$0",
            "\t]",
            ")"
        ],
        "description": "Log trace message to terminal"
    },
